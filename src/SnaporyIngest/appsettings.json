{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=snapory.db"
  },
  "S3": {
    "ServiceUrl": "https://s3.gra.io.cloud.ovh.net",
    "BucketName": "snapory-photos",
    "AccessKey": "PLACEHOLDER_ACCESS_KEY",
    "SecretKey": "PLACEHOLDER_SECRET_KEY"
  },
  "AiService": {
    "Url": "http://localhost:8000"
  },
  "Jwt": {
    "Secret": "PLACEHOLDER_JWT_SECRET_MUST_BE_REPLACED_VIA_ENV_OR_USER_SECRETS",
    "Issuer": "snapory-api",
    "Audience": "snapory-users",
    "ExpirationHours": 24
  },
  "Cors": {
    "Origins": "http://localhost:3000,http://localhost:3001"
  }
}